# Production Environment Variables for Coolify

# Application Environment
NODE_ENV=production

# API Configuration (Update with your actual backend URL)
VITE_API_URL=https://api.eyesonground.com

# Supabase Configuration (Update with your actual Supabase settings)
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Google Maps API (if used)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_key

# App Configuration
VITE_APP_NAME=EyesOnGround
VITE_APP_VERSION=1.0.0

# Analytics (if used)
VITE_GA_TRACKING_ID=your_google_analytics_id

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
